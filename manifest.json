{
    "short_name": "StackUnderflow",
    "name": "StackUnderflow",
    "version": "0.0.1",
    "manifest_version": 2,

    "permissions": 
    [
        "tabs",
        "activeTab",
        "storage",
        "*://www.google.com/search?*"
    ],
    
    "background": 
    {
        "scripts": ["background.js"],
        "persistent": false
    },
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    
    "browser_action": 
    {
        "default_title": "StackUnderflow",
        "default_popup": "popup.html"
    }
}